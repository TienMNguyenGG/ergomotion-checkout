        "e6afd73b-1d53-4520-9d39-5b5f020ba6b0": {
          "type": "item_features_banner",
          "settings": {
            "get_icon_url": true,
            "url_icon": "https:\/\/cdn.shopify.com\/s\/files\/1\/0662\/9516\/9245\/files\/Asset_7_e250f81b-f624-4f7e-9746-36573f50f6d9.svg?v=1670382696",
            "content_new": "<p>When idle, there is a max .05 draw from the outlet (equivalent to illuminating the clock on your oven)<\/p>"
          }
