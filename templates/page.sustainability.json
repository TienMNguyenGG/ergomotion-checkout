        "template--16588400001245__7a7e671e-4a42-4ff7-9a9c-1e12e87ba8f7-166660502233dc9f9a-0": {
          "type": "heading",
          "settings": {
            "heading": "Creating a near−zero carbon footprint",
            "heading_size": "h2"
