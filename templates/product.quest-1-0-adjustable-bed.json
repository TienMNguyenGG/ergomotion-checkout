        "324958df-e932-4022-8abc-b0e8ed188d9d": {
          "type": "item_grid",
          "settings": {
            "get_image_url": true,
            "url_image_update": "https:\/\/cdn.shopify.com\/s\/files\/1\/0662\/9516\/9245\/files\/Wired_Remote.svg?v=1666919718",
            "title": "Wired Remote",
            "content": "<p>Our two-button wired remote offers up-down ease of operation for quick and gentle adjustments that don't jolt your neck or spine.<\/p>"
